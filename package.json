{
  "name": "redux-universal-boilerplate",
  "description": "Boilerplate for universal React applications.",
  "license": "MIT",
  "version": "0.0.1",
  "main": "bin/server.js",
  "scripts": {
    "clean": "$(npm bin)/rimraf static/dist",
    "start": "NODE_ENV=production $(npm bin)/babel-node ./bin/server.js",
    "build": "npm run clean && NODE_ENV=production $(npm bin)/webpack --verbose --colors --display-error-details --config ./webpack/prod.config.js",
    "postinstall": "$(npm bin)/webpack --display-error-details --config ./webpack/prod.config.js",
    "test": "$(npm bin)/mocha test --recursive --require ./test/testHelper.js --compilers js:babel/register",
    "test:watch": "npm run test -- --watch",
    "test:cov": "$(npm bin)/babel-node $(npm bin)/isparta cover --report=html _mocha -- --recursive --require ./test/testHelper.js --compilers js:babel/register",
    "test:cov:open": "npm run test:cov && open ./coverage/index.html",
    "lint": "npm run lint:eslint && npm run lint:jscs && npm run lint:flow",
    "lint:flow": "$(npm bin)/flow",
    "lint:eslint": "$(npm bin)/eslint ./",
    "lint:jscs": "$(npm bin)/jscs ./",
    "dev": "npm run dev:server:webpack & npm run dev:server",
    "dev:server": "$(npm bin)/nodemon --exec $(npm bin)/babel-node ./bin/server.js",
    "dev:server:webpack": "BABEL_ENV=development-client $(npm bin)/babel-node ./webpack/webpack-dev-server.js"
  },
  "dependencies": {
    "autoprefixer-loader": "3.1.0",
    "babel": "5.8.23",
    "babel-core": "5.8.25",
    "babel-loader": "5.3.2",
    "babel-plugin-typecheck": "1.3.0",
    "babel-runtime": "5.8.25",
    "css-loader": "0.19.0",
    "express": "4.13.3",
    "extract-text-webpack-plugin": "0.8.2",
    "file-loader": "^0.8.4",
    "history": "1.12.2",
    "immutable": "^3.7.5",
    "is-promise": "^2.1.0",
    "isomorphic-fetch": "^2.1.1",
    "node-sass": "^3.3.3",
    "react": "0.14.0",
    "react-document-meta": "2.0.0-rc2",
    "react-dom": "0.14.0",
    "react-redux": "3.1.0",
    "react-router": "1.0.0-rc1",
    "redux": "3.0.2",
    "redux-async-middleware": "0.0.0",
    "redux-immutablejs": "0.0.6",
    "redux-multiplex": "0.0.2",
    "redux-universal": "0.0.2",
    "rimraf": "^2.4.3",
    "sass-loader": "3.0.0",
    "style-loader": "0.12.4",
    "url-loader": "0.5.6",
    "webpack": "1.12.2",
    "webpack-isomorphic-tools": "0.9.1"
  },
  "devDependencies": {
    "babel-eslint": "4.1.3",
    "babel-plugin-react-transform": "1.1.1",
    "chai": "^3.3.0",
    "eslint": "1.6.0",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "1.0.0",
    "eslint-plugin-react": "3.5.1",
    "flow-bin": "^0.17.0",
    "isparta": "^3.1.0",
    "istanbul": "^0.3.22",
    "jscs": "^2.2.1",
    "mocha": "^2.3.3",
    "nodemon": "^1.7.1",
    "react-addons-test-utils": "0.14.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "^1.1.1",
    "redux-logger": "^2.0.3",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0",
    "webpack-dev-middleware": "1.2.0",
    "webpack-hot-middleware": "2.4.1"
  }
}
